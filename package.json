{
  "name": "simple_webpack_boilerplate",
  "version": "1.0.0",
  "description": "A ready to use simple webpack boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --mode development  --hot --config webpack.config.js",
    "dev": "webpack-dev-server --mode development --progress --inline --hot  --colors --config webpack.dev.config.js",
    "build2": "webpack --mode production",
    "build": "webpack -p --config webpack.production.config.js --progress --profile --colors --display errors-only"
  },
  "author": "pinglinh",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "next",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint-loader": "lastest",
    "extract-text-webpack-plugin": "next",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "ip": "^1.1.5",
    "koa": "lastest",
    "koa-body": "lastest",
    "koa-static": "lastest",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "path": "^0.12.7",
    "postcss-loader": "next",
    "preload-webpack-plugin": "lastset",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "antd": "^3.4.1",
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "moment": "^2.22.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2"
  }
}
